# Vue应用环境配置

# API服务器地址 - 开发环境
VUE_APP_API_URL=http://localhost:3001/api

# 应用名称
VUE_APP_NAME=订单管理系统

# 应用版本
VUE_APP_VERSION=1.0.0

# 是否启用调试模式
VUE_APP_DEBUG=true

# 是否启用控制台日志
VUE_APP_ENABLE_CONSOLE_LOG=true

# 分页默认大小
VUE_APP_DEFAULT_PAGE_SIZE=20

# 文件上传最大大小（MB）
VUE_APP_MAX_FILE_SIZE=10

# 支持的图片格式
VUE_APP_SUPPORTED_IMAGE_FORMATS=jpg,jpeg,png,gif,webp

# JWT Token过期提醒时间（分钟）
VUE_APP_TOKEN_EXPIRE_WARNING_TIME=5